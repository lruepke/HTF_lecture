FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      permeability;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 0 0 0 0 0];

internalField   uniform 1e-14;

boundaryField
{
    seafloor{ type            zeroGradient; }
    bottom{ type            zeroGradient; }
    sidewalls{ type            zeroGradient; }
}


