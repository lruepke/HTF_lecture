FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


defaultFieldValues
(
    volScalarFieldValue permeability 5e-17

);

regions
(
    zoneToCell
    {
        name "intrusion";
        fieldValues
        (
            volScalarFieldValue permeability 1e-20
        );
    }
);

